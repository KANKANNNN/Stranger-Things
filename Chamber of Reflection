use_bpm 65
with_fx :echo, mix: 0.1, phase: 0.55 do
  sample :bd_mehackit, cutoff: 110
  sample :drum_cymbal_closed
  sleep 0.5
  sample :drum_cymbal_closed
  sleep 0.5
  sample :drum_cymbal_closed
  sample :drum_snare_hard
  sleep 0.5
  sample :drum_cymbal_closed
  sleep 0.25
  sample :bd_mehackit, cutoff: 110
  sleep 0.25
  sample :drum_cymbal_closed
  sample :bd_mehackit, cutoff: 110
  sleep 0.5
  sample :drum_cymbal_closed
  sleep 0.5
  sample :drum_snare_hard
  sample :drum_cymbal_closed
  sleep 0.5
  sample :drum_cymbal_closed
  sleep 0.5
  sample :bd_mehackit, cutoff: 110
  sample :drum_cymbal_closed
  sleep 0.5
  sample :drum_cymbal_closed
  sleep 0.5
  sample :drum_cymbal_closed
  sample :drum_snare_hard
  sleep 0.5
  sample :drum_cymbal_closed
  sleep 0.25
  sample :bd_mehackit, cutoff: 110
  sleep 0.25
  sample :drum_cymbal_closed
  sample :bd_mehackit, cutoff: 110
  sleep 0.5
  sample :drum_cymbal_closed
  sleep 0.25
  sample :drum_snare_hard
  sleep 0.25
  sample :drum_cymbal_closed
  sample :drum_snare_hard
  sleep 0.25
  sample :bd_mehackit, cutoff: 110
  sample :drum_cymbal_open, sustain: 1.5
  sleep 0.75
end
live_loop :heart do
  with_fx :reverb do
    with_fx :echo, mix: 0.1, phase: 0.55 do
      sample :bd_mehackit, cutoff: 110
      sleep 1.75
      sample :bd_mehackit, cutoff: 110
      sleep 0.25
      sample :bd_mehackit, cutoff: 110
      sleep 2
    end
  end
end
live_loop :hi do
  with_fx :reverb do
    with_fx :echo, mix: 0.1, phase: 0.55 do
      sample :drum_cymbal_closed
      sleep 0.5
    end
  end
end
live_loop :sn do
  with_fx :reverb do
    with_fx :echo, mix: 0.1, phase: 0.55 do
      sleep 1
      sample :drum_snare_hard
      sleep 1
    end
  end
end
