live_loop :back do
  with_fx :distortion do
    with_fx :reverb,mix: 0.6 do
      use_synth :hollow
      play:a4, sustain: 2, amp: 0.6
      play:a5, sustain: 2, amp: 0.6
      play:a7, sustain: 1.55, amp: 0.6
      sleep 2
      play:b4, sustain: 2, amp: 0.6
      play:b5, sustain: 2, amp: 0.6
      play:b7, sustain: 2, amp: 0.6
      sleep 2
      play:c5, sustain: 3.55, amp: 0.6
      play:c6, sustain: 3.55, amp: 0.6
      play:c8, sustain: 3.55, amp: 0.6
      sleep 4
      play:a4, sustain: 2, amp: 0.6
      play:a5, sustain: 2, amp: 0.6
      play:a7, sustain: 2, amp: 0.6
      sleep 2
      play:b4, sustain: 2, amp: 0.6
      play:b5, sustain: 2, amp: 0.6
      play:b7, sustain: 2, amp: 0.6
      sleep 2
      play:e5, sustain: 3.55, amp: 0.6
      play:e6, sustain: 3.55, amp: 0.6
      play:e8, sustain: 3.55, amp: 0.6
      sleep 4
    end
  end
end
